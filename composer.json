{
    "name": "alexanderwright/shoppingcart",
    "description": "Laravel Shoppingcart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "version": "2.6.1",
    "authors": [
        {
            "name": "Rob Gloudemans",
            "email": "info@robgloudemans.nl"
        },
        {
            "name": "Alexander Wright",
            "email": "alexander@silverfish-design.co.uk"
        }
    ],
    "require": {
        "illuminate/support": "6.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.*|| 5.6.* || 5.7.* || 5.8.* || 6.0.* || 6.18.*",
        "illuminate/session": "6.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.*|| 5.6.* || 5.7.* || 5.8.* || 6.0.* || 6.18.*",
        "illuminate/events": "6.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.*|| 5.6.* || 5.7.* || 5.8.* || 6.0.* || 6.18.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0 || ~6.0 || ~7.0 || ~8.0",
        "mockery/mockery": "~1.3.0",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Gloudemans\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gloudemans\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "suggest": {
        "gloudemans/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Gloudemans\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "Gloudemans\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
